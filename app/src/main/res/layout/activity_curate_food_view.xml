<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CurateFoodView">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="312dp"
        android:layout_height="0dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="482dp"
        android:text="@string/curateTitle"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004" />

    <View
        android:id="@+id/foodCard"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="109dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="109dp"
        android:background="@drawable/rectangle_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="63dp"
        android:text="@string/proteintip"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.909" />

    <TextView
        android:id="@+id/amtProtein"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginEnd="10dp"
        android:gravity="center"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toStartOf="@+id/amtFat"
        app:layout_constraintStart_toEndOf="@+id/amtCals"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.314" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/foodImage"
        android:layout_width="263dp"
        android:layout_height="277dp"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/foodCard"
        app:layout_constraintTop_toTopOf="@+id/foodCard"
        app:layout_constraintVertical_bias="0.312" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="60dp"
        android:gravity="center"
        android:text="@string/carbtip"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.692" />

    <TextView
        android:id="@+id/amtCarbs"
        android:layout_width="52dp"
        android:layout_height="26dp"
        android:gravity="center"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintStart_toStartOf="@+id/foodCard"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.315" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="250dp"
        android:gravity="center"
        android:text="@string/caltip"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/foodCard"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/amtCals"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginStart="74dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintEnd_toStartOf="@+id/amtProtein"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.314" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="67dp"
        android:gravity="center"
        android:text="@string/fattip"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/amtFat"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginEnd="162dp"
        android:gravity="center"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/amtProtein"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.315" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="280dp"
        android:layout_height="56dp"
        android:background="@drawable/rounded_text_box"
        android:gravity="center"
        android:text="@string/macroIdentifier"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_background"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/foodCard"
        app:layout_constraintTop_toTopOf="@+id/foodCard"
        app:layout_constraintVertical_bias="0.786" />


    <Button
        android:id="@+id/btnCurate"
        android:layout_width="241dp"
        android:layout_height="74dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#85F9B5"
        android:text="@string/curateButton"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foodCard" />

    <Button
        android:id="@+id/back2Input"
        android:layout_width="160dp"
        android:layout_height="38dp"
        android:backgroundTint="#85F9B5"
        android:text="@string/toinput"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.079" />

    <Button
        android:id="@+id/toDashboard"
        android:layout_width="199dp"
        android:layout_height="38dp"
        android:layout_marginBottom="7dp"
        android:backgroundTint="#85F9B5"
        android:text="@string/toMD"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.957"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

    <TextView
        android:id="@+id/txtFoodTitle"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:text="placeholder"
        android:textAlignment="center"
        android:textColor="#FAFAFA"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/foodCard"
        app:layout_constraintEnd_toEndOf="@+id/foodCard"
        app:layout_constraintStart_toStartOf="@+id/foodCard"
        app:layout_constraintTop_toTopOf="@+id/foodCard"
        app:layout_constraintVertical_bias="0.04" />

</androidx.constraintlayout.widget.ConstraintLayout>